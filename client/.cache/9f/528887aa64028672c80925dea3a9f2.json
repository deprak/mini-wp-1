         </svg>\n            <div v-if=\"openedToggles.includes(article._id)\" class=\"absolute bg-blue-500 top-0 right-0 text-white mt-8 p-2 overflow-auto z-10\">\n              <a href=\"\" @click.prevent=\"$emit('toEditor', article._id)\" class=\"flex items-center p-2 hover:bg-gray-800 text-white text-sm no-underline hover:no-underline block\"><i class=\"mr-1 fa fa-cog fa-fw\"></i>Edit</a>\n              <a href=\"#\" class=\"flex items-center p-2 hover:bg-gray-800 text-white text-sm no-underline hover:no-underline block\"><i class=\"mr-1 fa fa-cog fa-fw\"></i>Delete</a>\n            </div>\n          </div>\n        </div>\n        <div v-html=\"article.content\" class=\"text-gray-700 mb-2\">\n          \n        </div>\n        <a class=\"cursor-pointer text-gray-700 hover:text-gray-900 underline\">Read more</a>\n      </div>\n      <!-- Tags -->\n      <div class=\"px-6 py-4\">\n        <span v-for=\"tag in article.tags\" :key=\"tag\" class=\"inline-block bg-gray-200 rounded-full px-3 py-1 text-sm font-semibold text-gray-700 mr-2\">{{ tag }}</span>\n      </div>\n      <!-- End of Tags -->\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from '../../helpers/axios'\nimport Swal from 'sweetalert2'\nexport default {\n  name: 'SearchResult',\n  data () {\n    return {\n      articles: [],\n      openedToggles: []\n    }\n  },\n  props: ['search'],\n  methods: {\n    toggleMenu(articleId) {\n      if (!this.openedToggles.includes(articleId)) {\n        this.openedToggles.push(articleId)\n      } else {\n        let index = this.openedToggles.indexOf(articleId)\n        if (index > -1) {\n          this.openedToggles.splice(index, index + 1)\n        }\n      }\n      console.log(this.openedToggles[0], this.openedToggles[1], this.openedToggles[2])\n    },\n    doSearch() {\n      let url = ''\n      if (this.search.kind === 'all') {\n        url = `/articles/?q=${this.search.keyword}`\n      } else if (this.search.kind === 'tag') {\n        url = `/articles/?tag=${this.search.keyword}`\n      }\n      axios({\n        methods: 'get',\n        url,\n        headers: {\n          access_token: localStorage.getItem('access_token')\n        }\n      })\n        .then(({ data }) => {\n          this.articles = data\n        })\n        .catch(err => {\n          console.log(err)\n        })\n    }\n  },\n  created() {\n    this.doSearch()\n  }\n}\n</script>\n\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"ccc1b8e362d3ea454b56721f2d851fdd","cacheData":{"env":{}}}